    "laravel-mix": "^1.7.*",
    "lodash": "^4.17.4",
    "vue": "^2.5.13"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "font-awesome": "^4.7.0",
    "install": "^0.10.2",
    "jquery-colorbox": "^1.6.4",
    "pace": "0.0.4",
